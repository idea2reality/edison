    "files": [
        "./app.ts",
        "./common/common.ts",
        "./common/services/services.ts",
        "./common/services/user.service.ts",
        "./components/components.ts",
        "./components/sidenav/sidenav.controller.ts",
        "./components/sidenav/sidenav.directive.ts",
        "./components/sidenav/sidenav.service.ts",
        "./components/sidenav/sidenav.ts",
        "./components/toolbar/toolbar.directive.ts",
        "./components/toolbar/toolbar.ts",
        "./views/userDetail.controller.ts",
        "./views/views.ts",
        "../node_modules/gulp-typescript/node_modules/typescript/bin/lib.d.ts",
        "../typings/angularjs/angular-route.d.ts",
        "../typings/angularjs/angular.d.ts",
        "../typings/jquery/jquery.d.ts",
        "../typings/tsd.d.ts"
    ]
